<%= form_for @listing do |f| %>
<div>
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>

<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>

<div>
  <%= f.label :max_guest %>
  <%= f.number_field :max_guest %>
</div>

<div>
  <%= f.label :price %>
  <%= f.number_field :price %>
</div>

<p>
   <%= f.file_field :images, multiple: true %>
 </p>

<%= f.submit "Submit" %>
<% end %>
